{
  "name": "react-scalable",
  "private": true,
  "sideEffects": false,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch --coverage --coverageReporters=text",
    "test:coverage": "jest --coverage --coverageReporters=lcov",
    "ci:e2e": "start-server-and-test serve http://localhost:3002 test:e2e:record",
    "test:e2eopen": "cross-env VITE_API_URL=http://localhost:3000 && NO_PROXY=localhost cypress open",
    "serve": "serve -d -l 3002 -s dist",
    "serve:test": "vite --mode test",
    "cypress:open": "cypress open",
    "test:e2e": "NO_PROXY=localhost cypress run",
    "test:e2e:record": "cypress run --record",
    "tsc": "tsc --noEmit",
    "lint": "eslint ./ --ext .ts,.tsx",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@react-oauth/google": "^0.12.1",
    "@redux-devtools/extension": "3.3.0",
    "@reduxjs/toolkit": "1.9.7",
    "axios": "1.6.8",
    "clsx": "^2.1.0",
    "final-form": "4.20.10",
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-http-backend": "^2.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-final-form": "6.5.9",
    "react-helmet-async": "2.0.4",
    "react-hook-form": "^7.51.2",
    "react-i18next": "^14.1.0",
    "react-icons": "5.0.1",
    "react-redux": "9.1.0",
    "react-router": "6.22.3",
    "react-router-dom": "6.22.3",
    "react-toastify": "^10.0.5",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@hookform/devtools": "^4.3.1",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.0",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "26.0.15",
    "@types/node": "20.11.30",
    "@types/react": "18.2.73",
    "@types/react-dom": "18.2.22",
    "@types/react-helmet-async": "1.0.3",
    "@types/react-router": "5.1.20",
    "@types/react-router-dom": "5.3.3",
    "@types/webpack-env": "1.18.4",
    "@types/yup": "0.32.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.19",
    "cross-env": "^7.0.3",
    "cypress": "^13.7.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-testing-library": "6.2.0",
    "husky": "9.0.11",
    "identity-obj-proxy": "3.0.0",
    "isomorphic-fetch": "3.0.0",
    "jest": "26.6.0",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "nock": "13.5.4",
    "postcss": "8.4.38",
    "prettier": "2.8.8",
    "prismock": "1.30.1",
    "purgecss": "5.0.0",
    "rimraf": "5.0.5",
    "rollup-plugin-visualizer": "5.12.0",
    "sass": "1.72.0",
    "serve": "14.2.1",
    "start-server-and-test": "2.0.3",
    "tailwindcss": "3.4.3",
    "ts-jest": "26.5.6",
    "typescript": "4.9.5",
    "typescript-eslint": "^7.6.0",
    "vite": "5.2.6",
    "vite-tsconfig-paths": "4.3.2"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
